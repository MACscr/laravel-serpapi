{
    "name": "macscr/laravel-serpapi",
    "description": "Laravel Package for tracking Google laravel-serpapi",
    "keywords": [
        "laravel-serpapi"
    ],
    "homepage": "https://github.com/macscr/laravel-serpapi",
    "license": "MIT",
    "authors": [
        {
            "name": "MACscr",
            "email": "macscr@macscr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "google/apiclient": "^2.7",
        "tcdent/php-restclient": "^0.1.7"
    },
    "autoload": {
        "psr-4": {
            "Macscr\\LaravelSerpapi\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macscr\\LaravelSerpapi\\LaravelSerpapiServiceProvider"
            ],
            "aliases": {
                "SerpApiSearch": "Macscr\\LaravelSerpapi\\Facades\\SerpApiSearchFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
